{
    "name": "bittyphp/bitty-event-manager",
    "description": "PSR-14 Event Manager implementation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Kingsley",
            "email": "bizurkur@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "localheinz/composer-normalize": "^1.1",
        "phpstan/phpstan": "^0.10.7",
        "phpstan/phpstan-deprecation-rules": "^0.10.2",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpstan/phpstan-strict-rules": "^0.10.1"
    },
    "autoload": {
        "psr-4": {
            "Bitty\\EventManager\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitty\\Tests\\EventManager\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
